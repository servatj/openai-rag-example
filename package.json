{
  "name": "product-rag",
  "version": "1.0.0",
  "description": "RAG system for querying product documentation using OpenAI embeddings",
  "main": "index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "ask": "tsx src/cli.ts",
    "dev:server": "tsx watch src/server.ts",
    "dev:web": "npm --prefix web run dev",
    "dev": "concurrently -n server,web -c blue,magenta \"npm run dev:server\" \"npm run dev:web\"",
    "build:web": "npm --prefix web run build",
    "preview:web": "npm --prefix web run preview"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "openai": "^6.15.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
